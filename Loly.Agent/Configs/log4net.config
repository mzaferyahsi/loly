    <!--         <conversionPattern value="%-5level %logger [%property{NDC}] - %message%newline" /> -->
    <!--     </layout> -->
    <!-- </appender> -->

    <root>
        <level value="DEBUG" />
        <appender-ref ref="Console" />
        <appender-ref ref="ElasticSearchAppender"/>
        <!-- <appender-ref ref="UdpAppender"/> -->
        <!--<appender-ref ref="RollingLogFileAppender"/>-->
    </root>
</log4net>